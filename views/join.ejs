                           <div class="form-outline mb-4">
                             <input type="text" name="username" placeholder="Username" id="username" class="form-control text-light" required/>
                             <label class="form-label text-light"><i class="fa-solid fa-user"></i> Username</label>
                           </div>
                         
                           <!-- Password input -->
                           <div class="form-outline mb-4">
