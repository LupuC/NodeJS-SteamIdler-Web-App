                                 <span class="btn-close" style="cursor: pointer" onclick="this.parentElement.style.display='none';">&times;</span>
                               </div> 
                           <% } else if(regError == "Betakey is not valid!"){ %>
                              <div class="alert alert-danger" role="alert" data-mdb-color="danger">
                                 <i class="fa-solid fa-triangle-exclamation"></i> <%= regError %>
                                 <span class="btn-close" style="cursor: pointer" onclick="this.parentElement.style.display='none';">&times;</span>
                                 </div> 
                           <% }else{%>
                           
                           <% } %>
                           <h5 class="mb-5 px-md-2 display-6 ls-tight"> Register </h5>
